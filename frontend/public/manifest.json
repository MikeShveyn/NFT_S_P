{
  "name": "NFT Defender",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Checks the security of NFTs on Opensea",
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup",
    "default_icon": {
      "48": "logo192.png"
    }
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "contextMenus"
  ],
  "icons": {
    "48": "logo192.png"
  }
}
